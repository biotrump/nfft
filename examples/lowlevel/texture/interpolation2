# An interpolation example with the dubna1 grid.
DATA_PATH="interpolation/data/artificial2"
PROP_EXT="_int2"

cd data_creator
echo 50 | ./data_creator ../$DATA_PATH/grid_h ../$DATA_PATH/grid_r ../$DATA_PATH/omega prop$PROP_EXT > ../$DATA_PATH/x

# lÃ¤uft sehr lang...
cd ../interpolation
echo 50 | ./calculateOmega ../$DATA_PATH/x ../$DATA_PATH/grid_h ../$DATA_PATH/grid_r prop_om$PROP_EXT > ../$DATA_PATH/omega2
